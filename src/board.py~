class Board:
    def __init__(self):
       self.board = [[0 for x in range(8)] for y in range(8)]

    def setup_pieces(self):
        self.board[0] = ["R","N","B","Q","K","B","N","R"]
        self.board[1] = ["P"]*8
        print(self.board[0])
        print(self.board[1])
        print(self.board[2])

board1 = Board()
board1.setup_pieces()

